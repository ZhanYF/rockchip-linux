-				remote-endpoint = <&mipi_in_ucam>;
-				data-lanes = <1 2>;
-				clock-lanes = <0>;
-			};
-		};
-	};
-
 	// XXX: modem codec supplies:
 	// - vcc1v8_codec
 	// - vcca3v0_codec
