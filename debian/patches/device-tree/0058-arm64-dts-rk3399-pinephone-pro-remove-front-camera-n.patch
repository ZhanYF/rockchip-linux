-	status = "okay";
-
-	ports {
-		port@0 {
-			mipi_in_ucam: endpoint@0 {
-				reg = <0>;
-				remote-endpoint = <&ucam_out>;
-				data-lanes = <1 2>;
-			};
-		};
-	};
-};
-
 &isp0_mmu {
 	status = "okay";
 };
 
-&isp1_mmu {
-	status = "okay";
-};
-
-&mipi_dsi1 {
-	status = "okay";
-};
